/// @description Put in alarm[1] for dmg burst if-else
function scr_bullet_dmg_burst_alarm(argument0, argument1) {
	//args are the same from create script
	var d = argument0;
	var pop = argument1;

	if bullet_dmg = d
	{
	    bullet_dmg = 0
	    alarm[1] = pop;
	}
	else
	{
	    bullet_dmg = d;
	    alarm[1] = pop;
	}





}
